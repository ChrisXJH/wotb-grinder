{
    "extends": "airbnb",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "parser": "@babel/eslint-parser",
    "rules": {
        "prefer-destructuring": "off",
        "comma-dangle": "off",
        "arrow-parens": "off",
        "no-underscore-dangle": "off",
        "class-methods-use-this": "off",
        "camelcase": "off",
        "no-param-reassign": "off"
    }
}
